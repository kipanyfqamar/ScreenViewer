@model IEnumerable<ScreenViewer.Data.IDRServiceQueue>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ClientCallId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceQueue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClientId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactRecord.ClientCallId)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ClientCallId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceQueue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactRecord.ClientCallId)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id=item.IDRServiceQueueId }) |
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
