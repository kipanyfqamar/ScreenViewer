@model IEnumerable<ScreenViewer.Data.IDRServiceQueue>


    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ClientCallId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceQueue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ClientCallId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ServiceQueue)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedDate)
                </td>

                <td>
                    <a href="http://agent.screenplaycloud.com/@item.ServiceQueue/?CallID=12112&PreviousCallID=@item.ContactId&AgentName=Fred&UserId=60E81023-9942-40AC-BCCB-8B17BABCAC7E&ClientId=EEE35250-5337-4612-BC3C-189F6D8C4E39">Launch call</a><br />

                </td>
            </tr>
        }

    </table>

